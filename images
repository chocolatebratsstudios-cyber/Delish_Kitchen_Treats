    <script>
        // Tailwind script already loaded
        function initTailwind() {
            // Already configured
        }

        // Models data
        const models = [
            {name:"Nina Sky", img:"https://picsum.photos/id/1015/400/500", rate:"R49/min", loc:"Randburg"},
            {name:"Cherry Lips", img:"https://picsum.photos/id/1027/400/500", rate:"R39/min", loc:"Sandton"},
            {name:"Strawberry K", img:"https://picsum.photos/id/201/400/500", rate:"R59/min", loc:"Pretoria"},
            {name:"Lila Rose", img:"https://picsum.photos/id/64/400/500", rate:"R45/min", loc:"Midrand"},
            {name:"AI Vixen 7", img:"https://picsum.photos/id/1016/400/500", rate:"R29/min", loc:"Virtual"}
        ];

        function populateModels() {
            const grid = document.getElementById('models-grid');
            grid.innerHTML = '';
            models.forEach(m => {
                const card = document.createElement('div');
                card.className = 'bg-zinc-900 rounded-3xl overflow-hidden card-hover cursor-pointer';
                card.innerHTML = `
                    <img src="${m.img}" class="w-full h-80 object-cover">
                    <div class="p-5">
                        <div class="font-bold text-xl">${m.name}</div>
                        <div class="flex justify-between text-xs mt-3">
                            <div class="text-red-400">${m.rate}</div>
                            <div>${m.loc}</div>
                        </div>
                    </div>
                `;
                card.onclick = () => alert(`Opening profile for ${m.name}... (demo)`);
                grid.appendChild(card);
            });
        }

        // Shop items
        const shopItems = [
            {id:1, name:"Nina Sky Full Nude Set", price:299, type:"Photos"},
            {id:2, name:"Cherry Video 45min", price:799, type:"Video"},
            {id:3, name:"Strawberry Squirting Pack", price:449, type:"Bundle"}
        ];

        function populateShop() {
            const grid = document.getElementById('shop-grid');
            grid.innerHTML = '';
            shopItems.forEach(item => {
                const div = document.createElement('div');
                div.className = 'bg-zinc-900 rounded-3xl p-6 card-hover cursor-pointer';
                div.innerHTML = `
                    <div class="h-52 bg-zinc-800 rounded-2xl mb-6 flex items-center justify-center text-6xl">üçí</div>
                    <div class="font-semibold">${item.name}</div>
                    <div class="text-red-400 text-3xl mt-2">R${item.price}</div>
                    <button onclick="addToCart(${item.id});event.stopImmediatePropagation()" class="mt-6 w-full py-3 bg-red-600 rounded-2xl text-sm">ADD TO CART</button>
                `;
                grid.appendChild(div);
            });
        }

        // Cart
        let cart = [];
        function addToCart(id) {
            cart.push(id);
            document.getElementById('cart-count').textContent = cart.length;
            alert('Added to cart! (demo)');
        }
        function showCart() {
            if(cart.length === 0) {
                alert('Cart empty. Add some treats!');
                return;
            }
            alert('Cart total: R' + (cart.length * 299) + ' ‚Ä¢ Proceed to checkout (demo)');
        }

        // Call simulator
        let callTimer;
        let seconds = 0;
        function startPaidCall() {
            document.getElementById('call-modal').classList.remove('hidden');
            seconds = 0;
            updateTimer();
            callTimer = setInterval(() => {
                seconds++;
                updateTimer();
                if(seconds % 60 === 0) {
                    // fake charge
                }
            }, 1000);
        }
        function updateTimer() {
            const min = Math.floor(seconds/60).toString().padStart(2,'0');
            const sec = (seconds%60).toString().padStart(2,'0');
            document.getElementById('timer').textContent = `\( {min}: \){sec}`;
        }
        function endCall() {
            clearInterval(callTimer);
            document.getElementById('call-modal').classList.add('hidden');
            alert('Call ended. Charged R' + Math.floor(seconds/60 * 49) + ' ‚Ä¢ Thank you!');
        }

        // Booking
        function submitBooking() {
            const name = document.getElementById('name').value || 'Guest';
            alert(`Booking confirmed for ${name}!\nDate: ${document.getElementById('date').value}\nTime: ${document.getElementById('time').value}\n\nNina will WhatsApp you shortly.`);
        }

        // RedVelvet escort mock
        function populateEscorts() {
            const grid = document.getElementById('escort-grid');
            grid.innerHTML = `
                <div class="bg-zinc-900 rounded-3xl overflow-hidden">
                    <img src="https://picsum.photos/id/1005/400/500" class="w-full h-96 object-cover">
                    <div class="p-6">
                        <div class="flex justify-between">
                            <div class="font-bold">Mercedes</div>
                            <div class="text-green-400 text-sm">Randburg ‚Ä¢ Available</div>
                        </div>
                        <div class="text-xs mt-1">064 425 5149</div>
                        <button onclick="alert('Redirecting to WhatsApp...')" class="mt-6 w-full py-4 bg-red-600 rounded-2xl">BOOK NOW</button>
                    </div>
                </div>
                <!-- repeat 3 more similar -->
            `;
        }

        function searchEscorts() {
            alert('Showing 47 escorts in Johannesburg (demo)');
        }

        // Navigation
        function navigateTo(page) {
            document.querySelectorAll('section').forEach(s => s.classList.add('hidden'));
            if(page === 'home') document.getElementById('home').classList.remove('hidden');
            else if(page === 'models') document.getElementById('models').classList.remove('hidden');
            else if(page === 'live') document.getElementById('live').classList.remove('hidden');
            else if(page === 'shop') document.getElementById('shop').classList.remove('hidden');
            else if(page === 'book') document.getElementById('book').classList.remove('hidden');
            else if(page === 'redvelvet') {
                document.getElementById('redvelvet').classList.remove('hidden');
                populateEscorts();
            }
            window.scrollTo(0,0);
        }

        // Admin
        let adminOpen = false;
        function toggleAdmin() {
            adminOpen = !adminOpen;
            document.getElementById('admin-panel').classList.toggle('hidden', !adminOpen);
            if(adminOpen) initCharts();
        }

        // Fake charts
        function initCharts() {
            new Chart(document.getElementById('revenueChart'), {
                type: 'line',
                data: { labels: ['Mon','Tue','Wed','Thu','Fri','Sat','Sun'], datasets: [{ label:'Revenue ZAR', data:[12400,19800,23400,31200,45600,67200,87420], borderColor:'#c8102e', tension:0.4 }] },
                options: { plugins:{legend:{display:false}} }
            });
            new Chart(document.getElementById('modelChart'), {
                type: 'bar',
                data: { labels: ['Nina','Cherry','AI-7','Lila','Straw'], datasets: [{ label:'Sessions', data:[142,89,67,54,32], backgroundColor:'#ffd700' }] }
            });
        }

        // AI chat demo
        function sendAI() {
            const input = document.getElementById('ai-input');
            const chat = document.getElementById('ai-chat');
            if(!input.value) return;
            chat.innerHTML += `<div class="text-right"><span class="bg-red-900 px-4 py-2 rounded-3xl">${input.value}</span></div>`;
            setTimeout(() => {
                let reply = "All systems normal. Nina Sky has 12 new bookings today.";
                if(input.value.toLowerCase().includes("nina")) reply = "Nina Sky stats: 98% uptime ‚Ä¢ 47 calls today ‚Ä¢ Satisfaction 4.98‚òÖ";
                chat.innerHTML += `<div><span class="bg-zinc-800 px-4 py-2 rounded-3xl">${reply}</span></div>`;
                chat.scrollTop = chat.scrollHeight;
            }, 800);
            input.value = '';
        }

        // Fake login
        function showLogin() {
            alert('Welcome back! You are now logged in as VIP member (demo)');
        }

        // Join site
        function enterSite() {
            document.getElementById('agegate').style.display = 'none';
            populateModels();
            populateShop();
            // Auto show hero
        }

        // Keyboard shortcuts
        document.addEventListener('keydown', e => {
            if(e.key === '/' && document.getElementById('admin-panel').classList.contains('hidden')) toggleAdmin();
        });

        // Start everything
        window.onload = () => {
            // Show age gate first
            // All other init in enterSite()
            console.log('%cDELISH KITCHEN TREATS loaded ‚Ä¢ Fully functional demo', 'color:#c8102e;font-weight:bold');
        };
    </script>
